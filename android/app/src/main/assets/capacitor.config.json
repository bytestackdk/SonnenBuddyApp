{
	"appId": "dk.bytestack.sonnenbuddy",
	"appName": "Sonnen Buddy",
	"webDir": "www",
	"bundledWebRuntime": false,
	"server": {
		"clearText": true
	},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 2000,
			"launchAutoHide": true,
			"backgroundColor": "#002d2b",
			"androidSplashResourceName": "splash",
			"showSpinner": false,
			"splashFullScreen": true,
			"splashImmersive": true
		}
	},
	"cordova": {
		"accessOrigins": [
			"*"
		]
	}
}
